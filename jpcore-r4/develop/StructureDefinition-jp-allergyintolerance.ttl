       fhir:short [ fhir:v "Encounter when the allergy or intolerance was asserted. このアレルギー不耐症が判明した受療の状況（外来、入院、救急、在宅など）" ] ;
       fhir:definition [ fhir:v "The encounter when the allergy or intolerance was asserted.\r\n\r\nこのアレルギー不耐症が判明した受療の状況（外来、入院、救急、在宅など）" ] ;
       fhir:type ( [
         fhir:code [ fhir:v "Reference"^^xsd:anyURI ] ;
         fhir:targetProfile ( [
           fhir:v "http://jpfhir.jp/fhir/core/StructureDefinition/JP_Encounter"^^xsd:anyURI ;
           fhir:link <http://jpfhir.jp/fhir/core/StructureDefinition/JP_Encounter>
         ] )
       ] )
     ] [
       fhir:id [ fhir:v "AllergyIntolerance.onset[x]" ] ;
       fhir:path [ fhir:v "AllergyIntolerance.onset[x]" ] ;
       fhir:short [ fhir:v "When allergy or intolerance was identified. このアレルギー不耐症のオンセット" ] ;
       fhir:definition [ fhir:v "Estimated or actual date, date-time, or age when allergy or intolerance was identified.\r\n\r\nこのアレルギー不耐症が同定された事実もしくは推定された時期（日付、日時、年齢）。" ]
     ] [
       fhir:id [ fhir:v "AllergyIntolerance.recordedDate" ] ;
